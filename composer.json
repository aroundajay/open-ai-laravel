{
    "name": "orhanerday/open-ai",
    "description": "OpenAI GPT-3 Api Client in PHP with Laravel support",
    "keywords": [
        "orhanerday",
        "open-ai"
    ],
    "homepage": "https://github.com/orhanerday/open-ai",
    "license": "MIT",
    "authors": [
        {
            "name": "Orhan Erday",
            "email": "orhanerday@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Orhanerday\\OpenAi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orhanerday\\OpenAi\\Laravel\\OpenAiServiceProvider"
            ],
            "aliases": {
                "OpenAi": "Orhanerday\\OpenAi\\Laravel\\OpenAiFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orhanerday\\OpenAi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
